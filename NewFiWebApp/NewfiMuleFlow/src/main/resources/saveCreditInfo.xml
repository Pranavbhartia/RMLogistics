<LOXmlFormat version="1.0">
	<loan>
		<applicant id="applicantId">
			<credit craid="creditCardId" requestType="NEW" loginName="LOGIN_NAME"
				password="PASSWORD" equifax="equifaxStatus" experian="experianStatus"
				transunion="transunionStatus" />
			<field id="aBFirstNm">firstName</field>
			<field id="aBLastNm">lastName</field>
			<field id="aBAddr">applicantAddress</field>
			<field id="aBSsn">ssnNumber</field>
			<field id="aBCity">applicantCity</field>
			<field id="aBState">applicantState</field>
			<field id="aBZip">applicantZipCode</field>
		</applicant>
	</loan>
</LOXmlFormat>